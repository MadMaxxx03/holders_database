<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Classes.Question" table="QUESTION">
        <id name="qId" type="java.lang.Integer">
            <column name="QID"/>
            <generator class="identity"/>
        </id>
        <property name="question" type="string">
            <column name="QUESTION" length="200" not-null="true"/>
        </property>
        <property name="answer" type="string">
            <column name="ANSWER" length="50" not-null="true"/>
        </property>
        <property name="score" type="float">
            <column name="SCORE" not-null="true"/>
        </property>

        <many-to-one name="test" class="Classes.Test" column="tId"/>
    </class>
</hibernate-mapping>