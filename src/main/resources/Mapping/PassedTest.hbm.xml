<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Classes.PassedTest" table="PASSEDTEST">
        <id name="pId" type="java.lang.Integer">
            <column name="PID"/>
            <generator class="identity"/>
        </id>
        <property name="progress" type="float">
            <column name="PROGRESS" length="50" not-null="true"/>
        </property>
        
        <many-to-one name="holder" class="Classes.Holder" column="hId"/>

        <many-to-one name="test" class="Classes.Test" column="tId"/>

    </class>
</hibernate-mapping>